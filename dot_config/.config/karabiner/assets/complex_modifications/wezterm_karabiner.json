{
  "title": "Open WezTerm with Command+Enter",
  "rules": [
    {
      "description": "Command+Enter opens WezTerm",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "mandatory": ["command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "shell_command": "open -a WezTerm"
            }
          ]
        }
      ]
    }
  ]
}